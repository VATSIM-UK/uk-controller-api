{
    "name": "vatsim-uk/uk-controller-api",
    "description": "The web api backend to the UK Controller Plugin.",
    "keywords": ["vatsim", "uk", "ukcp", "uk-controller-api", "controller", "vatuk", "plugin"],
    "license": "GPL 3.0",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.0",
        "predis/predis": "^2.0",
        "laravel/passport": "^10.0",
        "laravel/framework": "^8.0",
        "knplabs/github-api": "^3.0",
        "nesbot/carbon": "^2.32",
        "maatwebsite/excel": "^3.1",
        "mjaschen/phpgeo": "^4.0",
        "laravel/horizon": "^5.7",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^3.0",
        "spatie/laravel-schedule-monitor": "^2.2",
        "ohdearapp/ohdear-php-sdk": "^3.1",
        "sentry/sentry-laravel": "^2.4",
        "composer/semver": "^3.2",
        "filament/filament": "^2.13"
    },
    "require-dev": {
        "fakerphp/faker": "^1.13",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/seeds",
            "database/migrations"
        ],
        "psr-4": {
            "App\\": ["app/" ,"tests/app/"],
            "util\\": ["tests/util"],
            "Database\\Factories\\": "database/factories/"
        }

    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "@php artisan filament:upgrade"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
