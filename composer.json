{
    "name": "vatsim-uk/uk-controller-api",
    "description": "The web api backend to the UK Controller Plugin.",
    "keywords": [
        "vatsim",
        "uk",
        "ukcp",
        "uk-controller-api",
        "controller",
        "vatuk",
        "plugin"
    ],
    "license": "GPL 3.0",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-intl": "*",
        "bugsnag/bugsnag-laravel": "^2.24",
        "composer/semver": "^3.2",
        "filament/filament": "^3.6.5",
        "guzzlehttp/guzzle": "^7.0",
        "knplabs/github-api": "^3.0",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.7",
        "laravel/passport": "^12.1",
        "laravel/socialite": "^5.5",
        "maatwebsite/excel": "^3.1",
        "mjaschen/phpgeo": "^4.0",
        "nesbot/carbon": "^3.0",
        "ohdearapp/ohdear-php-sdk": "^3.1",
        "predis/predis": "^2.0",
        "sentry/sentry-laravel": "^4.0",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-markdown": "^2.2",
        "spatie/laravel-schedule-monitor": "^3.0",
        "z3d0x/filament-logger": "^0.7.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "brianium/paratest": "^7.8",
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "11.5.50"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/seeds",
            "database/migrations"
        ],
        "psr-4": {
            "App\\": [
                "app/",
                "tests/app/"
            ],
            "util\\": [
                "tests/util"
            ],
            "Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
